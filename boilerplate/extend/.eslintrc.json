{
  "rules": {
    "global-require": "warn",
    "import/no-unresolved": "warn",
    "consistent-return": "warn",
    "prettier/prettier": [
      "error",
      {
        "printWidth": 100,
        "semi": false,
        "singleQuote": true,
        "tabWidth": 2
      }
    ]
  },
  "env": {
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
